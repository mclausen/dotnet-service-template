{
    "$schema": "http://json.schemastore.org/template",
    "author": "mclausen",
    "classifications": ["ServiceTemplate"],
    "name": "default-api-service-template",
    "description": "Creates a default dotnet api service",
    "identity": "TropicalCode.DefaultApiService.Csharp",
    "shortName": "api-service",
    "symbols": {
        "companyName": {
            "type": "parameter",
            "defaultValue": "MyCompany",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the Company",
            "replaces": "__Company__",
            "FileRename": "__Company__"
        },
        "departmentName": {
            "type": "parameter",
            "defaultValue": "MyDepartment",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the Department",
            "replaces": "__Department__",
            "FileRename": "__Department__"
        },
        "boundedContext": {
            "type": "parameter",
            "defaultValue": "MyBoundedContext",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the bounded context",
            "replaces": "__BoundedContext__",
            "FileRename": "__BoundedContext__"
        },
        "serviceName": {
            "type": "parameter",
            "defaultValue": "MyServiceName",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the Service",
            "replaces": "__ServiceName__",
            "FileRename": "__ServiceName__"
        }
    }
  }