{
    "$schema": "http://json.schemastore.org/template",
    "author": "mclausen",
    "classifications": ["ServiceTemplate"],
    "name": "default-api-service-template",
    "description": "Creates a default dotnet api service",
    "symbols": {
        "companyName": {
            "type": "parameter",
            "defaultValue": "MyCompany",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the Company",
            "replaces": "__Company_",
            "FileRename": "__Company_"
        },
        "departmentName": {
            "type": "parameter",
            "defaultValue": "MyDepartment",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the Department",
            "replaces": "__Department__",
            "FileRename": "__Department__"
        },
        "boundedContext": {
            "type": "parameter",
            "defaultValue": "BoundedContext",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the Service",
            "replaces": "_BoundedContext_",
            "FileRename": "_BoundedContext_"
        },
        "serviceName": {
            "type": "parameter",
            "defaultValue": "ServiceName",
            "isRequired": true,
            "datatype": "text",
            "description": "Provides the name of the Service",
            "replaces": "_ServiceName_",
            "FileRename": "_ServiceName_"
        }
    }
  }